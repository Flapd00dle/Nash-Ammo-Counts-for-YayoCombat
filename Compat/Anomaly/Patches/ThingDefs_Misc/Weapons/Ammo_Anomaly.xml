<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- Adding xml for magazine sizes and ammo costs for balance-->
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName = "Gun_HellcatRifle"]/weaponTags</xpath>
				<value>
					<li>ammo_industrial/30/1</li>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName = "Gun_Incinerator"]/weaponTags</xpath>
				<value>
					<li>ammo_none</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>